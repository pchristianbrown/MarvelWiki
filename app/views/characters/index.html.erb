<h1>MarvelWiki</h1>
<!-- <p>Find me in app/views/marvel_wiki/index.html.erb</p> -->
<%= form_for :article do |f| %>
  <p>
    <%= f.label :character %><br>
    <%= f.text_field :character %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>